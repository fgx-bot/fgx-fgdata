<?xml version="1.0"?>

<PropertyList>
 <path>cirrocumulus4.ac</path>

<animation>
		<type>material</type>
		<object-name>mesh</object-name>
		<emission>
			<red-prop>/rendering/scene/diffuse/red</red-prop>
	  		<green-prop>/rendering/scene/diffuse/green</green-prop>
	    		<blue-prop>/rendering/scene/diffuse/blue</blue-prop>
		</emission>
		<diffuse>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
		</diffuse>
		<ambient>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
		</ambient>
		<specular>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
		</specular>
		<shininess>0</shininess>
	</animation>

<animation>
  <object-name>mesh</object-name>
  <enable-hot type="bool">false</enable-hot>
</animation>

<effect>
  <inherits-from>Effects/cloud-models</inherits-from>
  <object-name>mesh</object-name>
</effect>

</PropertyList>
