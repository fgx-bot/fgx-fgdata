<?xml version="1.0"?>
<PropertyList>

    <name>Comm2</name>
    <w-base>255</w-base>
    <h-base>32</h-base>
    <w>255</w>
    <h>32</h>

    <layers>

        <layer>
            <name>comm2 used</name>
            <type>text</type>
            <font>led</font>
            <point-size>9</point-size>
            <color>
                <red>0.75</red>
                <green>0.24</green>
                <blue>0.05</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>instrumentation/comm[1]/frequencies/selected-mhz</property>
                    <scale>0.999992</scale>
                    <format>%04.2f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>-112</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-10</offset>
                </transformation>
            </transformations>
        </layer>

        <layer>
            <name>comm2 channel</name>
            <type>text</type>
            <font>led</font>
            <point-size>9</point-size>
            <color>
                <red>0.75</red>
                <green>0.24</green>
                <blue>0.05</blue>
            </color>
            <chunks>
                <chunk>
                    <type>number-value</type>
                    <property>instrumentation/comm[1]/frequencies/standby-mhz</property>
                    <scale>0.999992</scale>
                    <format>%04.2f</format>
                </chunk>
            </chunks>
            <transformations>
                <transformation>
                    <type>x-shift</type>
                    <offset>0</offset>
                </transformation>
                <transformation>
                    <type>y-shift</type>
                    <offset>-10</offset>
                </transformation>
            </transformations>
        </layer>
    </layers>
</PropertyList>
