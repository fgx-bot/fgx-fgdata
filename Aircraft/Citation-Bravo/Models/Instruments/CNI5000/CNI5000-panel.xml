            </condition>
            <x>174</x>
            <y>232</y>
            <w>255</w>
            <h>32</h>
        </instrument>

        <instrument include="kt70-1.xml">
            <name>ADF</name>
            <condition>
                <greater-than>
                    <property>systems/electrical/outputs/transponder</property>
                    <value>5</value>
                </greater-than>
            </condition>
            <x>199</x>
            <y>148</y>
            <w>255</w>
