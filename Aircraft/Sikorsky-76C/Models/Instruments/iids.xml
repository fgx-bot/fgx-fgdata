<?xml version="1.0"?>
<PropertyList>

    <path>iids.ac</path>

    <animation>
        <type>select</type>
        <object-name>tempsign</object-name>
        <condition>
            <less-than>
                <property>environment/temperature-degc</property>
                <value>0.0</value>
            </less-than>
        </condition>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>temp10</object-name>
        <property>environment/temperature-degc</property>
        <factor>0.01</factor>
        <step>10</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>textranslate</type>
        <object-name>temp1</object-name>
        <property>environment/temperature-degc</property>
        <factor>0.1</factor>
        <step>1</step>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!--  T5  -->

    <animation>
