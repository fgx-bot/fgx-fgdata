<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <key n="83">
    <name>S</name>
    <desc>Start smoke</desc>
    <binding>
      <command>property-assign</command>
      <property>controls/smoke</property>
      <value>true</value>
    </binding>
  </key>

  <key n="100">
   <name>d</name>
   <desc>Canopy</desc>
   <binding>
    <command>nasal</command>
    <script>doors.doorsystem.crewexport();</script>
   </binding>
  </key>

  <key n="117">
    <name>u</name>
    <desc>Stop smoke</desc>
    <binding>
      <command>property-assign</command>
      <property>controls/smoke</property>
      <value>false</value>
    </binding>
  </key>

</PropertyList>

