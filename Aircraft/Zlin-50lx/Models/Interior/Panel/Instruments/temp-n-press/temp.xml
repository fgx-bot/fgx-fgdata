<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

 <path>temp.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>Face</object-name>
    <object-name>Bezel</object-name>
    <object-name>temp.PtrT</object-name>
    <object-name>press1.PtrT</object-name>
    <object-name>press2.PtrT</object-name>
    <object-name>pastille1</object-name>
    <object-name>pastille2</object-name>
    <object-name>pastille3</object-name>

    <!-- Objets transparents -->
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>temp.PtrT</object-name>
    <object-name>press1.PtrT</object-name>
    <object-name>press2.PtrT</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>temp.PtrT</object-name>
    <property>/instrumentation/engine/oil-temp</property>
    <interpolation>
     <entry><ind>32</ind><dep>0</dep></entry>
     <entry><ind>248</ind><dep>120</dep></entry>
    </interpolation>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0.009</z-m>
    </center>
  </animation>

