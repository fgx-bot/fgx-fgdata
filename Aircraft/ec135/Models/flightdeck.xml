	<!--sideward animation backdoor right-->
	<animation>
		<type>translate</type>
		<object-name>backdoor_intre</object-name>
				
		<property>/sim/model/door-positions/ribackDoor/position-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>.1</ind>
				<dep>.05</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>.05</dep>
			</entry>
		</interpolation>
		<axis>
			<x>01</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<!--forward animation backdoor right-->
	<animation>
		<type>translate</type>
		<object-name>backdoor_intre</object-name>
		
		<property>/sim/model/door-positions/ribackDoor/position-norm</property>
		<interpolation>
