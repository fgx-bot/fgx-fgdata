<?xml version="1.0"?>

<!-- Sea Hawk model config file -->
<!-- Lee Elliott 2002/12/26 leee-fgfs@spatial.freeserve.co.uk -->

<PropertyList>
 <path>SeaHawk.ac</path>
 <offsets>
   <z-m>0.0</z-m>
   <pitch-deg>0</pitch-deg>
 </offsets>

<!-- Canopy -->
 <animation>
  <type>rotate</type>
  <object-name>Canopy</object-name>
  <property>/canopy/position-norm</property>
  <factor>4.0</factor>
  <center>
   <x-m>-2.0</x-m>
   <y-m>0.0</y-m>
   <z-m>-10.0</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>

<!-- Left Wing -->
 <animation>
  <type>rotate</type>
  <object-name>LUWing</object-name>
  <object-name>LLWing</object-name>
  <object-name>LUAileron</object-name>
  <object-name>LLAileron</object-name>
  <object-name>LFlap</object-name>
  <property>/canopy/position-norm</property>
  <factor>-108</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>-1.66</y-m>
   <z-m>0.1</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

<!-- Right Wing -->
 <animation>
  <type>rotate</type>
  <object-name>RUWing</object-name>
  <object-name>RLWing</object-name>
  <object-name>RUAileron</object-name>
  <object-name>RLAileron</object-name>
  <object-name>RFlap</object-name>
  <property>/canopy/position-norm</property>
  <factor>108</factor>
  <center>
   <x-m>0.0</x-m>
   <y-m>1.66</y-m>
   <z-m>0.1</z-m>
  </center>
  <axis>
   <x>1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

<!-- Rudder bits -->
 <animation>
  <type>rotate</type>
  <object-name>Rudder</object-name>
  <property>/surface-positions/rudder-pos-norm</property>
  <factor>-50</factor>
  <center>
   <x-m>5.38</x-m>
   <y-m>0.0</y-m>
   <z-m>0.0</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>

<!-- Elevator bits -->
 <animation>
  <type>rotate</type>
  <object-name>LUElevator</object-name>
  <object-name>LLElevator</object-name>
  <property>/surface-positions/elevator-pos-norm</property>
  <factor>40</factor>
  <center>
   <x-m>5.39</x-m>
   <y-m>0.0</y-m>
   <z-m>0.70</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

 <animation>
  <type>rotate</type>
  <object-name>RUElevator</object-name>
  <object-name>RLElevator</object-name>
  <property>/surface-positions/elevator-pos-norm</property>
  <factor>40</factor>
