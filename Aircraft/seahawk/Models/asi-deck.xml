<?xml version="1.0"?>

<!-- deck landing air speed indicator -->

<PropertyList>

	<path>asi-deck.ac</path>

	<animation>
		<type>rotate</type>
		<object-name>asi-Needle1</object-name>
		<property>/instrumentation/airspeed-indicator/indicated-speed-kt</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>80</ind>
				<dep>60</dep>
			</entry>
			<entry>
				<ind>90</ind>
				<dep>90</dep>
			</entry>
			<entry>
				<ind>100</ind>
				<dep>120</dep>
			</entry>
			<entry>
				<ind>110</ind>
				<dep>150</dep>
			</entry>
			<entry>
				<ind>120</ind>
				<dep>180</dep>
			</entry>
			<entry>
				<ind>130</ind>
				<dep>210</dep>
			</entry>
			<entry>
				<ind>140</ind>
				<dep>240</dep>
			</entry>
			<entry>
				<ind>150</ind>
				<dep>270</dep>
			</entry>
			<entry>
				<ind>750</ind>
				<dep>270</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- Panel Lighting -->
	<animation>
		<type>material</type>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>


	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
