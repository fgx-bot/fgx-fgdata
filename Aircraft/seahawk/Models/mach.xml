<?xml version="1.0"?>

<!-- Mach -->

<PropertyList>

	<path>mach.ac</path>

	<!-- Mach -->
	<animation>
		<type>rotate</type>
		<object-name>mach-Needle1</object-name>
		<property>/velocities/mach</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>30</dep>
			</entry>
			<entry>
				<ind>0.6</ind>
				<dep>90</dep>
			</entry>
			<entry>
				<ind>0.7</ind>
				<dep>150</dep>
			</entry>
			<entry>
				<ind>0.8</ind>
				<dep>210</dep>
			</entry>
			<entry>
				<ind>0.9</ind>
				<dep>270</dep>
			</entry>
			<entry>
				<ind>1.0</ind>
				<dep>330</dep>
			</entry>
			<entry>
				<ind>1.1</ind>
				<dep>390</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!-- Panel Lighting -->
	<animation>
		<type>material</type>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>

	<!-- *** LoD *** -->
	<animation>
		<type>range</type>
		<min-m>0</min-m>
		<max-m>5</max-m>
	</animation>

</PropertyList>
