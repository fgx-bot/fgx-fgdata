<?xml version="1.0"?>

<PropertyList>

<path>clock2.ac</path>

    <animation>
        <type>material</type>
        <object-name>Clock.face</object-name>
        <object-name>Clock.ring</object-name>
        <object-name>Hr.hand</object-name>
        <object-name>Mn.hand</object-name>
        <object-name>Sc.hand</object-name>
        <emission>
            <red>0.5</red>
            <green>0.2</green>
            <blue>0.0</blue>
            <factor-prop>systems/electrical/outputs/instrument-lights-norm</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Hr.hand</object-name>
        <property>instrumentation/clock/indicated-sec</property>
    <factor>0.008333</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Mn.hand</object-name>
        <property>instrumentation/clock/indicated-sec</property>
    <factor>0.1</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Sc.hand</object-name>
        <property>instrumentation/clock/indicated-sec</property>
    <factor>6</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.019</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Clock.ring</object-name>
        <property>instrumentation/clock/bezel-rotation-min</property>
    <factor>6</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>