 <animation>
  <type>translate</type>
  <object-name>fwheel.L</object-name>
  <object-name>fwheel2.L</object-name>
  <property>gear/gear[0]/compression-norm</property>
  <interpolation>
   <entry>
    <ind>0.0</ind>
    <dep>0</dep>
   </entry>
   <entry>
    <ind>1.00</ind>
    <dep>0.25</dep>
   </entry>
  </interpolation>
  <axis>
   <x>0</x>
   <y>0</y>
   <z>1</z>
  </axis>
 </animation>
